<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">
    <t t-name="sales_move_list.ListView.Buttons" t-inherit="web.ListView.Buttons" t-inherit-mode="extension">
        <xpath expr="//div[hasclass('o_list_buttons')]" position="inside">
            <!-- t-on-click="this.showUnFixedPrice" -->
            <button type="button" t-on-click="unfixedPriceBtnClickEvent" t-if="model.root.resModel == 'sale.order'" class="btn btn-primary" id="set_price">
                Set Current Price
            </button>
        </xpath>
    </t>
</templates>
